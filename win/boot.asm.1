start:
    mov ax, 0xb800
    mov ds, ax

    mov byte [0x00], 0x41
    mov byte [0x01], 0x0c

    mov byte [0x02], 's'
    mov byte [0x03], 0x04

    mov byte [0x04], 's'
    mov byte [0x05], 0x04

    mov byte [0x06], 'e'
    mov byte [0x07], 0x04

    mov byte [0x08], 'm'
    mov byte [0x09], 0x04

    mov byte [0x0a], 'b'
    mov byte [0x0b], 0x04

    mov byte [0x0c], 'l'
    mov byte [0x0d], 0x04

    mov byte [0x0e], 'y'
    mov byte [0x0f], 0x04

    mov byte [0x10], '.'
    mov byte [0x11], 0x04

    mov bx, 0x7c00+again
; 起循环作用
again:
    jmp bx ; 绝对间接近跳转，在一个段的内部跳转， 跳转的目标位置不是在指令中直接给出的，而是通过寄存器bx间接给出的，给出的地址是目标位置的实际偏移地址，是一个绝对地址
current:
    times 510-(current-start) db 0

    db 0x55, 0xaa 